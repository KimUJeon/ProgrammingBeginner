import java.io.*;

class Main{
	public static void main(String[] args) throws Exception{
		
		int a = 15;
		
		if ( a % 10 == 0){
			System.out.println("10의 배수입니다.");
		}
		
		else if( a % 3 == 0 && a % 5 == 0){
			System.out.println("3과 5의 배수입니다.");
		}
	
		else{
			System.out.println("else 문이 동작하였습니다.");
		}
	}
}


'''
3과 5의 배수입니다.
'''
