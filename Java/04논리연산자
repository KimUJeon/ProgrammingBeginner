# &&(AND) 연산자는 모두 true 일때만 true 값이 반환
# ||(OR) 연산자는 둘 중 하나만 true 면 true 반환
# !(NOT) 연산자는 true를 false, false를 true로 반환


import java.io.*;
class Main {
	public static void main(String[] args) throws Exception {
		
		System.out.println((4>3)&&(5==5)); 				// true 출력 예시
		
		System.out.println((3 < 2)||false); // false 출력
		
		System.out.println(!(4 != 4)); 		// true 출력

	}
}


'''
true
false
true
'''
